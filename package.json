{
  "name": "trustless-voting",
  "version": "0.0.1",
  "description": "Anonymous voting on Ethereum blockchain using zero knowledge proof and Account Abstraction",
  "scripts": {
    "prepare": "sh scripts/prepare.sh",
    "build": "parcel build public/index.html --public-url ./",
    "start": "parcel public/index.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy-local": "hardhat run --network localhost scripts/deploy.ts",
    "deploy-mumbai": "hardhat run --network mumbai scripts/deploy.ts",
    "deploy-fuji": "hardhat run --network fuji scripts/deploy.ts",
    "deploy-goerli": "hardhat run --network goerli scripts/deploy.ts",
    "deploy-polygon": "hardhat run --network polygonMainnet scripts/deploy.ts"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://trustless-democracy.github.io/trustless-voting",
  "dependencies": {
    "@zerodevapp/social-wallet": "^1.0.1",
    "@zerodevapp/wagmi": "^1.0.19",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "copy-to-clipboard": "^3.3.3",
    "ethers": "^5.7.2",
    "html5-qrcode": "^2.3.4",
    "parcel": "^2.8.2",
    "qrcode": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.12.10",
    "zk-merkle-tree": "^1.0.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@parcel/transformer-vue": "^2.8.2",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@wixc3/react-board": "^2.1.3",
    "assert": "^2.0.0",
    "buffer": "^5.7.1",
    "chai": "^4.3.7",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.0.3",
    "events": "^3.3.0",
    "gh-pages": "^5.0.0",
    "hardhat": "^2.12.6",
    "hardhat-gas-reporter": "^1.0.8",
    "os-browserify": "^0.3.0",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "solidity-coverage": "^0.8.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.9.4",
    "url": "^0.11.0"
  }
}
